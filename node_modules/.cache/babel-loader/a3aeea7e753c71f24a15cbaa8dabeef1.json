   let save = function () {\n            emit('edit-techno', technoToEdit.value);\n            technoToEdit.value = null;\n        };\n\n        return {\n            deleteTechno,\n            editTechno,\n            save,\n            technoToEdit,\n        };\n    },\n};\n</script>\n\n<style>\n\n    ul {\n    list-style: none;\n    width: 50%;\n    margin-left: 40%;\n    text-align: left;\n    }\n</style>"],"sourceRoot":""}]}